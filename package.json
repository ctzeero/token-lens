  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "conf": "^13.1.0",
    "inquirer": "^9.3.7",
    "sql.js": "^1.14.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.13.5",
    "esbuild": "^0.25.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
