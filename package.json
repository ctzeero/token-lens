{
  "name": "tokenlens",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": {
    "tlens": "./dist/index.js"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/index.js",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "package": "npm run build && pkg .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pkg": {
    "targets": [
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-linux"
    ],
    "outputPath": "dist/bin",
    "assets": [
      "logo.txt"
    ]
  },
  "keywords": [],
